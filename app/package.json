{
	"name": "app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"turso": "turso dev --db-file turso/dev.db",
		"drizzle:studio": "drizzle-kit studio --config drizzle.config.ts",
		"drizzle:generate": "drizzle-kit generate:sqlite --config drizzle.config.ts",
		"drizzle:push": "drizzle-kit push:sqlite --config drizzle.config.ts",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"dotenv": "^16.4.1",
		"drizzle-kit": "^0.20.13",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@fontsource-variable/plus-jakarta-sans": "^5.0.19",
		"@libsql/client": "^0.4.0",
		"@lucia-auth/adapter-drizzle": "^1.0.0",
		"abort-controller": "^3.0.0",
		"bits-ui": "^0.15.1",
		"cheerio": "^1.0.0-rc.12",
		"clsx": "^2.1.0",
		"cross-fetch": "^4.0.0",
		"drizzle-orm": "^0.29.3",
		"formsnap": "^0.4.2",
		"lucia": "^3.0.1",
		"lucide-svelte": "^0.314.0",
		"mode-watcher": "^0.1.2",
		"oslo": "^1.0.2",
		"svelte-sonner": "^0.3.11",
		"sveltekit-superforms": "^1.13.4",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.1.20",
		"zod": "^3.22.4"
	}
}
